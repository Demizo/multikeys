<Window x:Class="MultikeysGUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MultikeysGUI.View.Controls"
        mc:Ignorable="d"
        Title="MainWindow" Height="573" Width="1410">
    <Grid>
        <DockPanel>
            <ScrollViewer Width="1080">
                <StackPanel x:Name="KeyboardStack" Margin="5" />
            </ScrollViewer>
            <local:CommandPanel x:Name="SummaryPanel" DockPanel.Dock="Right" Margin="10" Width="300" Height="500"/>
        </DockPanel>
    </Grid>
</Window>
