<UserControl x:Class="MultikeysGUI.Pages.Controls.CommandPanel"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:MultikeysGUI.Pages.Controls"
             mc:Ignorable="d" 
             d:DesignHeight="200" d:DesignWidth="200"
             MinHeight="200" MinWidth="200">
    <Grid>
        <Rectangle Margin="0,0,0,0" Fill="White" Opacity="0.7" />
        <Border BorderThickness="3" BorderBrush="Black" />
        <TextBlock x:Name="lblTitle" Margin="10, 10, 10, 0" VerticalAlignment="Top" TextWrapping="Wrap" Text="TypeOfCommand"/>
        
    </Grid>
</UserControl>
